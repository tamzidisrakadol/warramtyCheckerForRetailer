<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".views.Dashboard_Activity">

    <ImageButton
        android:id="@+id/dashboardSellBtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="60dp"
        android:layout_marginTop="336dp"
        android:background="@android:color/transparent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/selling" />

    <View
        android:id="@+id/view"
        android:layout_width="@dimen/_2sdp"
        android:layout_height="@dimen/_65sdp"
        android:layout_marginStart="20dp"
        android:background="@color/black"
        app:layout_constraintStart_toEndOf="@+id/dashboardSellBtn"
        app:layout_constraintTop_toTopOf="@+id/dashboardSellBtn" />

    <ImageButton
        android:id="@+id/dashboardProfileBtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="336dp"
        android:background="@android:color/transparent"
        android:src="@drawable/user"
        app:layout_constraintStart_toEndOf="@+id/view"
        app:layout_constraintTop_toTopOf="parent" />


    <View
        android:layout_width="@dimen/_2sdp"
        android:layout_height="@dimen/_65sdp"
        android:layout_marginStart="20dp"
        android:background="@color/black"
        app:layout_constraintStart_toEndOf="@+id/dashboardProfileBtn"
        app:layout_constraintTop_toTopOf="@+id/dashboardProfileBtn" />

</androidx.constraintlayout.widget.ConstraintLayout>